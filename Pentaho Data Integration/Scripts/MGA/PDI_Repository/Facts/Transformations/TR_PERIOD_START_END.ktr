  <step>
    <name>JS: Calculate Months &amp; Dates</name>
    <type>ScriptValueMod</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <compatible>N</compatible>
    <optimizationLevel>9</optimizationLevel>
    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Get the minimum date

var dbStartDate;
var dbEndDate;
var varMinStartDate = getVariable("FSBI_MIN_DATE_TO_LOAD","");
var varMaxEndDate = getVariable("FSBI_MAX_DATE_TO_LOAD","");
var varSummariesToCurrentDate = getVariable("FSBI_SUMMARIES_TO_CURRENT_DATE","N");
var minStartDate;
var maxEndDate;

//varMinStartDate = "2009/03/01 00:00:00.000";
//varMaxEndDate = "2017/11/30 00:00:00.000";

// Use the min load date if it is set
if (varMinStartDate.length > 0) {
	minStartDate = str2date(varMinStartDate, "yyyy/MM/dd HH:mm:ss.sss");
}

// Use the max load date if it is set
if (varMaxEndDate.length > 0) {
	maxEndDate = str2date(varMaxEndDate, "yyyy/MM/dd HH:mm:ss.sss");
}

// Get minimum date

if (isDate(minStartDate)) {
	dbStartDate = minStartDate;
} else {
	if (POLICYSTARTDATE &amp;&amp; CLAIMSTARTDATE) {
  		if (POLICYSTARTDATE &lt; CLAIMSTARTDATE)
    		dbStartDate = POLICYSTARTDATE
  		else
    		dbStartDate = CLAIMSTARTDATE;
	} else {
		if (POLICYSTARTDATE)
  			dbStartDate = POLICYSTARTDATE
    	else 
			dbStartDate = CLAIMSTARTDATE;
	}
}

// Get the maximum date

if (isDate(maxEndDate)) {
	dbEndDate = maxEndDate;
} else {
	if (POLICYENDDATE &amp;&amp; CLAIMENDDATE) {
  		if (POLICYENDDATE > CLAIMENDDATE)
    		dbEndDate = POLICYENDDATE
  		else
    		dbEndDate = CLAIMENDDATE;
	} else {
		if (POLICYSTARTDATE)
  			dbEndDate = POLICYENDDATE
    	else 
			dbEndDate = CLAIMENDDATE;
	}
}

// Run summaries to current date.  If "Y" and dbEndDate is 
// greater than the current date then use the current date.
 if (varSummariesToCurrentDate.equals("Y") &amp;&amp; sys_currentDate.getTime() &lt; dbEndDate.getTime()) 
 	dbEndDate = sys_currentDate;


// Calculate the number of months between
// the start date (sDate) and end date (eDate)

var months = dateDiff(dbStartDate,dbEndDate,"m")+1;


// Calculate the first day of the month for the start date
var startMonth = month(dbStartDate);
var startYear = year(dbStartDate); 
var startDate = new Date(startYear, startMonth, 1);

// Calculate the last day of the month for the end date
var endMonth = month(dbEndDate); 
var endYear = year(dbEndDate);
var endDate = new Date(endYear, endMonth+1, 0);</jsScript_script>
      </jsScript>
    </jsScripts>
