    <sql>SELECT POLICYDATES.POLICYSTARTDATE, POLICYDATES.POLICYENDDATE, 
       CLAIMDATES.CLAIMSTARTDATE, CLAIMDATES.CLAIMENDDATE
FROM 
 (SELECT 1 AS ID,
         MIN(PT_ACCOUNTINGDATE) AS POLICYSTARTDATE,        
         MAX(PT_ACCOUNTINGDATE) AS POLICYENDDATE
  FROM STG_POLICYTRANSACTION
  WHERE SOURCE_SYSTEM='${SOURCE_SYSTEM}' and LOADDATE = ${sql_loadDate}
 ) POLICYDATES,
 (SELECT 1 AS ID,
         MIN(CT_ACCOUNTINGDATE) AS CLAIMSTARTDATE,
         MAX(CT_ACCOUNTINGDATE) AS CLAIMENDDATE
  FROM STG_CLAIMTRANSACTION
  WHERE SOURCE_SYSTEM='${SOURCE_SYSTEM}' and LOADDATE = ${sql_loadDate}
 ) CLAIMDATES
WHERE POLICYDATES.ID = CLAIMDATES.ID


