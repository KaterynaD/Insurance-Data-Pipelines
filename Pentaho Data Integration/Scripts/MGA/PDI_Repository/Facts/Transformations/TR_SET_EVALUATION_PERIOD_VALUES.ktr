    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// Sets the sql_periodStartDate and sql_periodEndDate based
// on the database defined in the G_DATABASE global
// variable in the kettle.properties file

var sql_periodStartDate;
var sql_periodEndDate;
var sql_processingDate;

var tmp_database = getVariable("FSBI_DATABASE", "MSSQL");


// Also, sets processingDate to the earlier of two values:
// currentDate or the periodEndDate.  For purposes of determining
// if a policy is inforce or expired, use this date.

var processingDate;


if (periodEndDate.getTime() &lt; currentDate.getTime())
{
	processingDate = periodEndDate;
}
else
{
	processingDate = currentDate;
}
       

// ** MSSQL **

if (tmp_database == "MSSQL") {
	sql_periodStartDate = "'" + date2str(periodStartDate,"yyyy/MM/dd HH:mm:ss.SSS") + "'";
	sql_periodEndDate = "'" + date2str(periodEndDate,"yyyy/MM/dd HH:mm:ss.SSS") + "'";
	sql_processingDate = "'" + date2str(processingDate,"yyyy/MM/dd") + "'";
}


// ** MYSQL **
if (tmp_database == "MYSQL") {
	sql_periodStartDate = "STR_TO_DATE('" + date2str(periodStartDate,"yyyy-MM-dd HH:mm:ss") + "', '%Y-%m-%d %h:%i:%s')";
	sql_periodEndDate = "STR_TO_DATE('" + date2str(periodEndDate,"yyyy-MM-dd HH:mm:ss") + "', '%Y-%m-%d %h:%i:%s')";
	sql_processingDate = "STR_TO_DATE('" + date2str(processingDate,"yyyy-MM-dd") + "', '%Y-%m-%d %h:%i:%s')";
}               


//Fix for the new version
var periodStartDate_str = date2str(periodStartDate,"yyyy-MM-dd")
var periodEndDate_str = date2str(periodEndDate,"yyyy-MM-dd")

</jsScript_script>
