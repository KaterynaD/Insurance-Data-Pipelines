    <optimizationLevel>9</optimizationLevel>
    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#x2f;&#x2f;Script here&#xd;&#xa;&#xd;&#xa;var path &#x3d; &#x22;&#x22;&#x3b;&#xd;&#xa;var proc_count &#x3d; &#x22;&#x22;&#x3b;&#xd;&#xa;var RS_nodes &#x3d; &#x22;&#x22;&#x3b;&#xd;&#xa;&#xd;&#xa;proc_count&#x3d;getVariable&#x28;&#x22;FSBI_PROC_COUNT&#x22;,&#x22;&#x22;&#x29;&#x3b;&#xd;&#xa;RS_nodes&#x3d;getVariable&#x28;&#x22;FSBI_DB_REDSHIFT_NODE_COUNT&#x22;,&#x22;&#x22;&#x29;&#x3b;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f; Redshift node count greater than 1, split to multiple files for parrallel processing&#xd;&#xa;&#xd;&#xa;if &#x28;&#x28;RS_nodes&#x3e;1&#x29; &#x26;&#x26; &#x28;&#x28;proc_count&#x21;&#x3d;null&#x29; &#x7c;&#x7c; &#x28;proc_count&#x21;&#x3d;0&#x29;&#x29;&#x29; &#x7b;&#xd;&#xa;		&#x2f;&#x2f; Row count null or zero, empty table - path 0 will skip file creation steps later in job.&#xd;&#xa;		if &#x28;trim&#x28;ROW_COUNT&#x29;&#x3d;&#x3d;null &#x7c;&#x7c; trim&#x28;ROW_COUNT&#x29;&#x3c;1&#x29; &#x7b;&#xd;&#xa;				path&#x3d;0&#x3b;&#xd;&#xa;		&#x7d;&#xd;&#xa;&#xd;&#xa;		&#x2f;&#x2f; Row count less than 250k&#x3a; 1 file, no need for multiple copies&#xd;&#xa;		else if &#x28;trim&#x28;ROW_COUNT&#x29;&#x3e;0 &#x26;&#x26; trim&#x28;ROW_COUNT&#x29;&#x3c;250000&#x29; &#x7b;&#xd;&#xa;				path&#x3d;1&#x3b;&#xd;&#xa;		&#x7d;&#xd;&#xa;&#xd;&#xa;		&#x2f;&#x2f; Row count greater than 249k less than 1 mill - start two copies&#xd;&#xa;		else if &#x28;trim&#x28;ROW_COUNT&#x29;&#x3e;249999 &#x26;&#x26; trim&#x28;ROW_COUNT&#x29;&#x3c;1000000&#x29; &#x7b;&#xd;&#xa;				&#x2f;&#x2f; 2 or less Processors - start one copy&#xd;&#xa;				if &#x28;&#x28;proc_count&#x29;&#x3c;3&#x29; &#x7b;&#xd;&#xa;					path&#x3d;1&#x3b;&#xd;&#xa;				&#x7d;&#xd;&#xa;				&#x2f;&#x2f; 3 or more Processors - start two copies&#xd;&#xa;				else &#x7b;&#xd;&#xa;					path&#x3d;2&#x3b;&#xd;&#xa;				&#x7d;&#xd;&#xa;		&#x7d;&#xd;&#xa;&#xd;&#xa;		&#x2f;&#x2f; Row count greater than 1 mill&#xd;&#xa;		else if &#x28;trim&#x28;ROW_COUNT&#x29;&#x3e;999999&#x29; &#x7b;&#xd;&#xa;				&#xd;&#xa;				if &#x28;&#x28;proc_count&#x29;&#x3c;3&#x29; &#x7b;&#xd;&#xa;					path&#x3d;1&#x3b;&#xd;&#xa;				&#x7d;&#xd;&#xa;&#xd;&#xa;				else &#x7b;&#xd;&#xa;					path&#x3d;&#x28;&#x28;proc_count&#x29;-2&#x29;&#x3b;&#xd;&#xa;				&#x7d;&#xd;&#xa;		&#x7d;&#xd;&#xa;&#xd;&#xa;&#x3b;&#x7d;&#xd;&#xa;&#xd;&#xa;&#x2f;&#x2f; Redshift node count 1 &#x2f;&#x2f; null or no proc count set in config.properties - do not split any files&#xd;&#xa;else  &#x7b;&#xd;&#xa;	&#x2f;&#x2f; Row count null or zero - skip&#xd;&#xa;		if &#x28;trim&#x28;ROW_COUNT&#x29;&#x3d;&#x3d;null &#x7c;&#x7c; trim&#x28;ROW_COUNT&#x29;&#x3c;1&#x29; &#x7b;&#xd;&#xa;				path&#x3d;0&#x3b;&#xd;&#xa;		&#x7d;&#xd;&#xa;&#xd;&#xa;		&#x2f;&#x2f; Row count greater than 1, but only single node in Redshift&#xd;&#xa;		else &#x7b;&#xd;&#xa;				path&#x3d;1&#x3b;&#xd;&#xa;		&#x7d;&#xd;&#xa;&#xd;&#xa;&#xd;&#xa;&#x3b;&#x7d;&#xd;&#xa;		&#xd;&#xa;			</jsScript_script>
