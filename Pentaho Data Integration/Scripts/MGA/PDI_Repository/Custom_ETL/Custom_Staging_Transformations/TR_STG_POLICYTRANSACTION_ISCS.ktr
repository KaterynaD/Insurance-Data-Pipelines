    <sql>select 						&#xd;&#xa;  &#x27;&#x24;&#x7b;SOURCE_SYSTEM&#x7d;&#x27; as source_system&#xd;&#xa;, &#x24;&#x7b;sql_loadDate&#x7d; as loaddate,		&#xd;&#xa;  ifnull&#x28;h.SystemId, h_r.SystemId&#x29;	SystemId&#xd;&#xa;, ps.policyRef  as policy_uniqueID	  &#xd;&#xa;, case when ps.StatSequenceReplace is null then ps.StatSequence else concat&#x28;ps.StatSequence,&#x27;-&#x27;,ps.StatSequenceReplace&#x29; end as policyTransaction_uniqueID											&#xd;&#xa;, ifnull&#x28;ps.RiskId, &#x27;Unknown&#x27;&#x29; as primaryrisk_uniqueID		&#xd;&#xa;, ifnull&#x28;ps.AssignedDriver, &#x27;Unknown&#x27;&#x29; as secondaryrisk_uniqueID										&#xd;&#xa;, replace&#x28;concat&#x28;cast&#x28;ps.policyRef as char&#x29; , &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;case ps.LineCd when &#x27;Liability&#x27; then &#x27;BusinessOwner&#x27; else ps.LineCd end,&#x27;&#x7e;&#x27;&#x29; , &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;ps.RiskCd,&#x27;&#x7e;&#x27;&#x29; , &#x27;-&#x27; 						&#xd;&#xa;   ,coalesce&#x28;ps.CoverageCd,ps.FeeCd,&#x27;&#x7e;&#x27;&#x29; , &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;ps.CoverageItemCd,&#x27;&#x7e;&#x27;&#x29; , &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;ps.RateAreaName,&#x27;&#x7e;&#x27;&#x29; , &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;case when ps.LineCd &#x3d; &#x27;PersonalAuto&#x27; and ps.AnnualStatementLineCd &#x3d; &#x27;051&#x27; then &#x27;211&#x27; else ps.AnnualStatementLineCd end,&#x27;&#x7e;&#x27;&#x29; , &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;ps.SublineCd,&#x27;&#x7e;&#x27;&#x29;, &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;ps.Deductible1,&#x27;&#x7e;&#x27;&#x29;, &#x27;-&#x27;  						&#xd;&#xa;   ,ifnull&#x28;ps.Deductible2,&#x27;&#x7e;&#x27;&#x29;, &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;ps.Limit1,&#x27;&#x7e;&#x27;&#x29;, &#x27;-&#x27; 						&#xd;&#xa;   ,ifnull&#x28;ps.Limit2,&#x27;&#x7e;&#x27;&#x29;						&#xd;&#xa;   &#x29;,&#x27; &#x27;,&#x27;&#x7e;&#x27;&#x29;						&#xd;&#xa;  as coverage_uniqueID						&#xd;&#xa;, date_format&#x28;ps.BookDt,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29; as pt_transactionDate						&#xd;&#xa;, ps.TransactionNumber as pt_sequence						&#xd;&#xa;, date_format&#x28;ps.AccountingDt,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29; as pt_accountingDate						&#xd;&#xa;, date_format&#x28;ps.TransactionEffectiveDt,&#x27;&#x25;Y-&#x25;m-&#x25;d&#x27;&#x29; as pt_effectiveDate																&#xd;&#xa;,  case 						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Cancellation&#x27; then 						&#xd;&#xa;	   case 					&#xd;&#xa;	     when ps.TransactionEffectiveDt &#x3d; ps.EffectiveDt then &#x27;CF&#x27;					&#xd;&#xa;	     else &#x27;CM&#x27;					&#xd;&#xa;	   end  					&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Endorsement&#x27; then &#x27;EN&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Expire&#x27; then &#x27;EXP&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;New Business&#x27; then &#x27;NB&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Non-Renewal&#x27; then &#x27;NR&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Non-Renewal Rescind&#x27; then &#x27;NRR&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Reinstatement&#x27; then &#x27;RI&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Reinstatement With Lapse&#x27; then &#x27;RI&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Renewal&#x27; then &#x27;RB&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Rewrite-Renewal&#x27; then &#x27;RB&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Cancellation - Company&#x27; then &#x27;CM&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Cancellation - Insured&#x27; then &#x27;CM&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Cancellation - NonPay&#x27; then &#x27;CM&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Other&#x27; then &#x27;EN&#x27;						&#xd;&#xa;     when ps.TransactionCd &#x3d;&#x27;Unapply&#x27; then &#x27;EN&#x27;						&#xd;&#xa;	 when ps.TransactionCd &#x3d;&#x27;Reapply&#x27; then &#x27;EN&#x27;					&#xd;&#xa;	 when ps.TransactionCd &#x3d;&#x27;Commission Reversal&#x27; then &#x27;EN&#x27;					&#xd;&#xa;   end as pt_typeCode&#xd;&#xa;, &#x27;&#x27; 	pt_typeSubCode	&#xd;&#xa;, case						&#xd;&#xa;	when ps.FeeCd is null then null					&#xd;&#xa;	else &#x27;FS&#x27;					&#xd;&#xa; end as pt_typeCode_Fees				&#xd;&#xa;, ifnull&#x28;ps.FeeCd,&#x27;&#x27;&#x29; as pt_typeSubCode_Fees																											&#xd;&#xa;, ps.WrittenPremiumAmt as pt_amount			&#xd;&#xa;, ifnull&#x28;ps.WrittenPremiumFeeAmt,0&#x29; as WrittenPremiumFeeAmt								&#xd;&#xa;, ps.WrittenCommissionAmt as pt_commissionAmount												&#xd;&#xa;, ps.InforceChangeAmt as pt_termAmount												&#xd;&#xa;, ps.TransactionCd						&#xd;&#xa;, case when ps.FeeCd is null or ps.InforceChangeAmt &#x3c;&#x3e; 0 then &#x27;Yes&#x27; else &#x27;No&#x27; end as IsPremiumTransaction						&#xd;&#xa;, case when ps.FeeCd is not null then &#x27;Yes&#x27; else &#x27;No&#x27; end as IsFeeTransaction		&#xd;&#xa;from prodcse.PolicyStats ps	&#xd;&#xa;&#x23;join prodcse_dw.BasicPolicy bp&#xd;&#xa;&#x23;on bp.SystemId&#x3d;ps.PolicyRef&#xd;&#xa;&#x23;and bp.cmmContainer&#x3d;&#x27;Policy&#x27;	&#xd;&#xa;left outer join etl.stg_policyhistory h&#xd;&#xa;on ps.PolicyRef&#x3d;h.PolicyRef&#xd;&#xa;and ps.BookDt&#x3d;h.BookDt&#xd;&#xa;and ps.TransactionNumber&#x3d;h.TransactionNumber&#xd;&#xa;and ps.StatSequenceReplace is null&#xd;&#xa;left outer join prodcse.PolicyStats ps_r&#xd;&#xa;on ps.StatSequenceReplace&#x3d;ps_r.Systemid&#xd;&#xa;left outer join etl.stg_policyhistory h_r&#xd;&#xa;on ps_r.PolicyRef&#x3d;h_r.PolicyRef&#xd;&#xa;and ps_r.BookDt&#x3d;h_r.BookDt&#xd;&#xa;and ps_r.TransactionNumber&#x3d;h_r.TransactionNumber&#xd;&#xa;and ps_r.StatSequenceReplace is null&#xd;&#xa;where ps.BookDt  &#x3e; &#x24;&#x7b;sql_bookDate&#x7d; and ps.BookDt  &#x3c;&#x3d; cast&#x28;&#x24;&#x7b;sql_currentDate&#x7d; as da